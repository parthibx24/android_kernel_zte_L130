		#clock-cells = <0>;
		reg = <0 0x402d0234 0 0x4>;	/* select reg */
		sprd,mux-msk = <0x7>;
		clocks = <&clk_rco_4m>, <&ext_26m>, <&clk_rco_25m>,
			 <&clk_twpll_38m4>, <&clk_twpll_51m2>;
