	clk_gpll: clk@4040009c {
		compatible = "sprd,sc9850-adjustable-pll-clock";
		#clock-cells = <0>;
		reg = <0 0x4040009c 0 0x4>,
			<0 0x404000a0 0 0x4>;
		clocks = <&clk_gpll_gate 0>;
