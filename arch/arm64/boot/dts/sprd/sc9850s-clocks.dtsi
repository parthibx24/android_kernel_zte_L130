	/* AON clock */
	clk_aon_apb: clk@402d0230 {
		compatible = "sprd,composite-clock";
		#clock-cells = <0>;
		reg = <0 0x402d0230 0 0x4>;	/* select reg and divider reg */
		sprd,mux-msk = <0x7>;
		sprd,div-msk = <0x300>;
		clocks = <&clk_rco_25m>, <&ext_26m>, <&ext_rco_100m>,
			 <&clk_twpll_96m>, <&clk_twpll_128m>, <&clk_twpll_153m6>;
		clock-output-names = "clk_aon_apb";
	};

