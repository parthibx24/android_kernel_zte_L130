&adi_bus {
	pmic: pmic@0 {
		compatible = "sprd,sc2731";
		reg = <0>; /* chip select 0 */
		spi-max-frequency = <26000000>;
		#address-cells = <1>;
		#size-cells = <0>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupts = <GIC_SPI 31 IRQ_TYPE_LEVEL_HIGH>;
		sprd,pmic_intc_base = <0x140>;
		sprd,pmic_intc_irqmax = <16>;

		pmic_charger: charger@0{
			compatible = "sprd,sc2731-charger";
			reg = <0x0>;
			status = "disabled";
		};

		pmic_timer: timer@40{
			compatible = "sprd,sc2731-chg-timer";
			reg = <0x40>;
			interrupt-parent = <&pmic>;
			interrupts = <9 IRQ_TYPE_LEVEL_HIGH>;
		};

		pmic_fchg: fast-charger@60{
			compatible = "sprd,sc2731-fchg";
			reg = <0x60>;
			interrupt-parent = <&pmic>;
			interrupts = <6 IRQ_TYPE_LEVEL_HIGH>;
			status = "disabled";
		};

		pmic_wdt: watchdog@80 {
			compatible = "sprd,sc2731-wdt";
			reg = <0x80>;
		};

		pmic_chg_wdt: watchdog@c0 {
			compatible = "sprd,sc2731-chg-wdt";
			reg = <0xc0>;
		};

		pmic_typec: typec@100 {
			compatible = "sprd,typec";
			interrupt-parent = <&pmic>;
			interrupts = <13 IRQ_TYPE_LEVEL_HIGH>;
			reg = <0x100>;
			mode = <2>;
			tsleep = <1500>;
			ext-ldo-sw = <0>;
			status = "disabled";
		};

		pmic_bltc_rgb: bltc-rgb@200 {
			compatible = "sprd,sc2731-bltc-rgb";
			reg = <0x200>;
			status = "disabled";
		};

		pmic_flash: flash@240 {
			compatible = "sprd,sc2731-flash";
			interrupt-parent = <&pmic>;
			interrupts = <15 IRQ_TYPE_LEVEL_HIGH>;
			reg = <0x240>;
		};


		pmic_rtc: rtc@280 {
			compatible = "sprd,sc2731-rtc";
			interrupt-parent = <&pmic>;
			interrupts = <2 IRQ_TYPE_LEVEL_HIGH>;
			reg = <0x280>;
		};

		pmic_kpled: kpled@2c4 {
			compatible = "sprd,sc2731-kpled";
			brightness_max = <255>;
			brightness_min = <0>;
			run_mode = <1>; /* default current mode */
			reg = <0x2c4>;
			status = "disabled";
		};

		pmic_eic: gpio-controller@300 {
			compatible = "sprd,sc2731-eic";
			reg = <0x300>;
			interrupts = <5 IRQ_TYPE_LEVEL_HIGH>;
			interrupt-parent = <&pmic>;
			gpio-controller;
			#gpio-cells = <2>;
			/* need to be replaced by gpio-ranges */
			sprd,gpiobase = <304>;
			sprd,ngpios = <16>;
			interrupt-controller;
			#interrupt-cells = <2>;
		};

		pmic_efuse: efuse@380 {
			compatible = "sprd,sc2731-efuse";
			reg = <0x380>;
			sprd,block-num = <32>;
			sprd,block-width = <16>;
			hwlocks = <&hwslock1 12>;
			hwlock-names = "pmic_efuse";
		};

		pmic_thermal: pmic-thermal@400 {
			compatible = "sprd,sc2731-thermal";
			#thermal-sensor-cells = <1>;
			reg = <0x400>;
		};

		pmic_adc: adc@480{
			compatible = "sprd,sc2731-adc";
			reg = <0x480>;
			#io-channel-cells = <1>;
			hwlocks = <&hwslock1 4>;
			hwlock-names = "pmic_adc";
		 };

		sprd_audio_codec_ana: audio-codec@700 {
			compatible = "sprd,sc2731-audio-codec";
			#sound-dai-cells = <1>;
			reg = <0x700>;
			interrupts = <8 IRQ_TYPE_NONE>;
			digital-codec = <&sprd_audio_codec_dig>;
			status = "disabled";
		};

		power-controller@810 {
			compatible = "sprd,sc2731-regulator";
			reg = <0xc28>,<0xcd4>,<0xd00>,<0xd1c>,<0xd20>,<0xd3c>;

			regulators {
				#address-cells = <1>;
				#size-cells = <0>;

				vddarm0: dcdc@0 {
					reg = <0>;
					regulator-name = "vddarm0";
					sprd,default-microvolt = <900000>;
					sprd,step-microvolt = <3125>;
					regulator-min-microvolt = <400000>;
					regulator-max-microvolt = <1996000>;
					regulator-microvolt-offset = <0>;
					sprd,hide-offset = <1000>;
					sprd,default-on;
					sprd,auto_step;
				};

				vddarm1: dcdc@1 {
					reg = <1>;
					regulator-name = "vddarm1";
					regulator-cal-channel = <0x0 0x0 0x0>;
					sprd,default-microvolt = <900000>;
					sprd,step-microvolt = <3125>;
					regulator-min-microvolt = <400000>;
					regulator-max-microvolt = <1996000>;
					regulator-microvolt-offset = <0>;
					sprd,hide-offset = <1000>;
					sprd,default-on;
					sprd,auto_step;
				};

				vddcore: dcdc@2 {
					reg = <2>;
					regulator-name = "vddcore";
					regulator-cal-channel = <0x0 0x0 0x0>;
					sprd,default-microvolt = <900000>;
					sprd,step-microvolt = <3125>;
					regulator-min-microvolt = <400000>;
					regulator-max-microvolt = <1996000>;
					regulator-microvolt-offset = <0>;
					sprd,hide-offset = <1000>;
					sprd,default-on;
					sprd,auto_step;
				};

				vddgpu: dcdc@3 {
					reg = <3>;
					regulator-name = "vddgpu";
					regulator-cal-channel = <0x0 0x0 0x0>;
					sprd,default-microvolt = <900000>;
					sprd,step-microvolt = <3125>;
					regulator-min-microvolt = <400000>;
					regulator-max-microvolt = <1996000>;
					regulator-microvolt-offset = <0>;
					sprd,hide-offset = <1000>;
					sprd,default-on;
					sprd,auto_step;
				};

				vddmem: dcdc@4 {
					reg = <4>;
					regulator-name = "vddmem";
					regulator-cal-channel = <0x0 0x0 0x0>;
					sprd,default-microvolt = <1000000>;
					sprd,step-microvolt = <3125>;
					regulator-min-microvolt = <600000>;
					regulator-max-microvolt = <3596000>;
